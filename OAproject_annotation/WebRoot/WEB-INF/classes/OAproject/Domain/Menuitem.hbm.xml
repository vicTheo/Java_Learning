<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	
 <class name="OAproject.Domain.Menuitem" table="menueitem">
   <id name="mid" type="java.lang.Long" length="5">
   <column name="mid"></column>
   <generator class="assigned"></generator>
   </id>
   <property name="icon" type="java.lang.String" length="100"></property>
   <property name="name" type="java.lang.String" length="10"></property>
   <property name="isParent" type="java.lang.Boolean" length="10"></property>
   <property name="pid" type="java.lang.Long" length="5"></property>
   
   <set name="users" table="user_menuitem">
   <key><column name="mid"></column></key>
   <many-to-many class="OAproject.Domain.User" column="uid"></many-to-many>
   </set>
 </class>
</hibernate-mapping>